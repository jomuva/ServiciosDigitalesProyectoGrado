@model ServiciosDigitalesProy.Models.Elemento

@{
    ViewBag.Title = " Solicitudes";
    ViewBag.SubSeccion = "Elemento Solicitud  Cliente " + ViewBag.NombreCliente;
}

<h3>@ViewBag.SubSeccion</h3>
<div class="col-lg-12">
    <!--breadcrumbs start -->
    <ul class="breadcrumb">
        <li><a href="#"><i class="fa fa-th-list"></i>@ViewBag.Title</a></li>
        <li class="active"><a href="#">Elemento Solicitud</a></li>
    </ul>
    <!--breadcrumbs end -->
</div>
<section class="panel">
    <div class="panel-body">
        <div class="tab-content">
            <div id="home-3" class="tab-pane active">
                <!-- page start-->
                <div class="row">
                    <div class="col-lg-12">
                        <section class="panel">
                            <header class="panel-heading tab-bg-dark-navy-blue tab-right ">
                                <div class="row">
                                </div>
                                <ul class="nav nav-tabs pull-right">
                                    <li class="active">
                                        <a data-toggle="tab" href="#home-3">
                                            <i class="fa fa-circle" aria-hidden="true"></i>
                                            @ViewBag.SubSeccion
                                        </a>
                                    </li>
                                </ul>

                            </header>
                            <div class="col-lg-12">
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        Elemento 
                                    </div>
                                    <!-- /.panel-heading -->
                                    <div class="panel-body">
                                        <table width="100%" class="table table-striped table-bordered table-hover" id="table-inicial">
                                            <thead>
                                                <tr>
                                                    <td>
                                                        <b>Descripción</b>
                                                    </td>
                                                    <td>
                                                        <b>Serial</b>
                                                    </td>
                                                    <td>
                                                        <b>Modelo</b>
                                                    </td>
                                                    <td>
                                                        <b>Marca</b>
                                                    </td>
                                                    <th></th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>
                                                        @Html.DisplayFor(x => x.tipoElemento.Descripcion)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(x => x.serial)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(x => x.modelo)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(x => x.marca)
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <!-- /.panel-body -->
                                </div>
                                <div class="form-group">
                                    <div class="col-md-2">
                                        @Html.ActionLink("Volver", "VolverDetallesProducto", new  { @*d = Model.id_producto*@ }, new { @class = "btn btn-primary" })
                                    </div>
                                </div>
                                <!-- /.panel -->
                            </div>
                        </section>
                    </div>
                </div>
                <!-- page end-->
            </div>
        </div>
    </div>
</section>
@if ((string)TempData["mensaje"] != null || (string)TempData["mensaje"] != "")
{
    <script>
        $(function (msg) {
            $.bootstrapGrowl('@((string)TempData["mensaje"])', {
                type: '@((string)TempData["estado"])',
                offset: { from: 'top', amount: 100 }
            });
        });
    </script>
}

